
public class InsertSort {
	
	public static int[] insertionSort(int [] array){	
	int temp;
	
	for (int i=1; i<array.length ; i++){
		for ( int j=1; j>array.length; j--){
				if (array [j]<array [i-1]){
					temp= array[j];
					array[j]=array[j-1];
					array[j]= temp;
				}
		}}
				return array;
		}

}
